FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1e-8 1e-8 1e-8);

boundaryField
{
    top
    {
        type		noSlip;
    }

    bottom
    {
        type		noSlip;
    }

    left
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    right
    {
        type		outletInlet;
        outletValue	uniform (1 0 0);
    }

    front
    {
        type		noSlip;
    }

    back
    {
        type		noSlip;
    }

    cylinder
    {
        type		noSlip;
    }
}


